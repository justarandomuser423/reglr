make player be "zane"
make health be 100
make score be 0

change health to 75
change score to 10
say player
say health
say score

make bonus be 5 + 3 * 2
say bonus
change score to score + bonus
say score

if score above 15
    say "high score!"

repeat 3 times
    say "looping..."
    change score to score + 1
say score

make greet do
    say "hello"
    change score to score + 5

greet
